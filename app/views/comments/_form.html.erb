<%= form_for([@photo, @photo.comments.build]) do |f| %>
        <p class = "acomment">
            <%= f.label :"Name" %><br>
            <%= f.text_field :commenter %>
        </p>
        <p class = "acomment">
              
            <%= f.label :"Comment" %><br>
            <%= f.text_area :body %>
        </p>
        <p class= "acomment">
            <%= f.submit %>
        </p>
    <% end %>