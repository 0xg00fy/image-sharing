<%= form_for([@photo, @photo.comments.build]) do |f| %>
        <p>
            <%= f.label :"Name" %><br>
            <%= f.text_field :commenter %>
        </p>
        <p>
              
            <%= f.label :"Comment" %><br>
            <%= f.text_area :body %>
        </p>
        <p class = "commentsub">
            <%= f.submit %>
        </p>
    <% end %>