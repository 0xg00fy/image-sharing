<div class="page-header"><h1>Photos</h1></div>

  <!-- Renders image <div class="media">-->
  <% @photos.each do |photo| %>
    <div class="media-left">
      <!-- Renders image -->
      <%= link_to image_tag(photo.image.url, class: 'media-object', size: "400x300"), photo_path(photo) %>
    </div>
  <% end %>
