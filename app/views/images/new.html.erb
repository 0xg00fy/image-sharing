<h1>Upload a new Image</h1>

<%= form_for :image do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
 <p>
     <%= f.label :Upload %>
     <br>
     <%= f.file_field :picture %>
 </p>
 
 
  <p>
    <%= f.label :comment %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>